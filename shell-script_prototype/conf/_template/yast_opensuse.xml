<!--
* pre-scripts (very early, before anything else really happens)
* postpartitioning-scripts (after partitioning and mounting to /mnt but before RPM installation)
* chroot-scripts (after the package installation, before the first boot)
* post-scripts (during the first boot of the installed system, no services running)
* init-scripts (during the first boot of the installed system, all services up and running)
-->
<!-- log's directory: /var/admin/autoinstall/logs/ -->
  <scripts t="map">
    <!-- *** pre-scripts *************************************************** -->
    <pre-scripts t="list">
      <script t="map">
        <debug t="boolean">false</debug>
        <feedback t="boolean">false</feedback>
        <feedback_type/>
        <filename>zzz_pre-scripts.sh</filename>
        <interpreter>shell</interpreter>
        <location/>
        <notification/>
        <param-list t="list"/>
        <rerun t="boolean">false</rerun>
        <source><![CDATA[
  set -eu
  echo "zzz_pre-scripts.sh"
  mkdir -p /var/admin/autoinst/
  {
    if [ -n "${debug:-}" ] || [ -n "${debugout:-}" ]; then
      set -x
      export -p
    fi
    _FILE_SEED=""
    _COMD_LINE="$(cat /proc/cmdline)"
    for __LINE in ${_COMD_LINE:-}
    do
      case "${__LINE}" in
        preseed/url=*  | url=*  ) _FILE_SEED="${__LINE#*url=}";;
        preseed/file=* | file=* ) _FILE_SEED="${__LINE#*file=}";;
        ds=nocloud*             ) _FILE_SEED="${__LINE#*ds=nocloud*=}";_FILE_SEED="${_FILE_SEED%%/}/user-data";;
        inst.ks=*               ) _FILE_SEED="${__LINE#*inst.ks=}";;
        autoyast=*              ) _FILE_SEED="${__LINE#*autoyast=}";;
        inst.auto=*             ) _FILE_SEED="${__LINE#*inst.auto=}";;
        *) echo "${__LINE:-}";;
      esac
    done
    __DIRS="${_FILE_SEED%/user-data}"
    __DIRS="${__DIRS%/*/*}"
    __DIRS="${__DIRS%%/}"
    cd /var/admin/autoinst/
    echo "download/copy: [${__DIRS:-}]"
    case "${__DIRS:-}" in
      http:*|https:*|ftp:*|tftp:*) curl -L -# -O -R -S "${__DIRS:-}/script/autoinst_cmd_early.sh";;
      hd:sr0:*|cdrom|cdrom:*     ) cp -p "/mnt/install/repo/${__DIRS#*:/}/script/autoinst_cmd_early.sh" ./;;
      hd:*|dvd:*|cd:*            )
        if [[ -e /var/log/YaST2/y2start.log ]]; then
          while read -r __LINE
          do
            case "${__LINE:-}" in
              *AutoYaST=*)
                __SEED="${__LINE%%=*}"
                __SEED="${__LINE#"${__SEED:-}="}"
                __SEED="${__SEED#\"}"
                __SEED="${__SEED%\"}"
                __SEED="${__SEED#*://}"
                ;;
              *autoyast=*)
                __YAST="${__LINE%%=*}"
                __YAST="${__LINE#"${__YAST:-}="}"
                __YAST="${__YAST#\"}"
                __YAST="${__YAST%\"}"
                __YAST="${__YAST#*:/}"
                ;;
              *) ;;
            esac
          done < /var/log/YaST2/y2start.log
          __DEVS="${__SEED%"/${__YAST:-}"}"
          if [[ -n "${__DEVS:-}" ]] && [[ -e "/dev/${__DEVS}" ]]; then
            mkdir -p /media
            mount -r "/dev/${__DEVS}" /media
            cp -p "/media/${__DIRS#*:/}/script/autoinst_cmd_early.sh" ./
            umount /media
          fi
        fi
        ;;
      device:*                   ) echo "${__DIRS}";;
      label:*                    ) echo "${__DIRS}";;
      usb:*                      ) echo "${__DIRS}";;
      smb:*                      ) echo "${__DIRS}";;
      nfs:*                      ) echo "${__DIRS}";;
      hmc                        ) echo "${__DIRS}";;
      file:*|/*                  ) cp -p "${__DIRS#*:*//}/script/autoinst_cmd_early.sh" ./;;
      *) echo "${__DIRS:-}";;
    esac
    if [[ -e ./autoinst_cmd_early.sh ]]; then
      chmod +x ./autoinst_cmd_early.sh
      ./autoinst_cmd_early.sh
    fi
  } | tee -a /var/admin/autoinst/zzz_pre-scripts.sh.log 2>&1
]]>
        </source>
      </script>
    </pre-scripts>
    <!-- *** postpartitioning-scripts ************************************** -->
    <postpartitioning-scripts t="list">
      <script t="map">
        <debug t="boolean">false</debug>
        <feedback t="boolean">false</feedback>
        <feedback_type/>
        <filename>zzz_postpartitioning-scripts.sh</filename>
        <interpreter>shell</interpreter>
        <location/>
        <notification/>
        <param-list t="list"/>
        <rerun t="boolean">false</rerun>
        <source><![CDATA[
  set -eu
  echo "zzz_postpartitioning-scripts.sh"
  mkdir -p /mnt/var/admin/autoinst/
  {
    if [ -n "${debug:-}" ] || [ -n "${debugout:-}" ]; then
      set -x
      export -p
    fi
    cp -pv /var/admin/autoinst/* /mnt/var/admin/autoinst/
  } | tee -a /mnt/var/admin/autoinst/zzz_postpartitioning-scripts.sh.log 2>&1
]]>
        </source>
      </script>
    </postpartitioning-scripts>
    <!-- *** chroot-scripts ************************************************ -->
    <chroot-scripts t="list">
      <script t="map">
        <chrooted t="boolean">true</chrooted>
        <debug t="boolean">false</debug>
        <feedback t="boolean">false</feedback>
        <feedback_type/>
        <filename>zzz_chroot-scripts.sh</filename>
        <interpreter>shell</interpreter>
        <location/>
        <notification/>
        <param-list t="list"/>
        <rerun t="boolean">false</rerun>
        <source><![CDATA[
  set -eu
  echo "zzz_chroot-scripts.sh"
  {
    /var/admin/autoinst/autoinst_cmd_late.sh
  } | tee -a /var/admin/autoinst/zzz_chroot-scripts.sh.log 2>&1
]]>
        </source>
      </script>
    </chroot-scripts>
    <!-- *** post-scripts ************************************************** -->
    <post-scripts t="list">
      <script t="map">
        <debug t="boolean">false</debug>
        <feedback t="boolean">false</feedback>
        <feedback_type/>
        <filename>zzz_post-scripts.sh</filename>
        <interpreter>shell</interpreter>
        <location/>
        <notification/>
        <param-list t="list"/>
        <rerun t="boolean">false</rerun>
        <source><![CDATA[
  set -eu
  echo "zzz_post-scripts.sh"
  __DIRS="/mnt/var/admin/autoinst"
  [[ ! -e "${__DIRS:-}"/. ]] && __DIRS="${__DIRS#/mnt}"
  {
    if [ -n "${debug:-}" ] || [ -n "${debugout:-}" ]; then
      set -x
      export -p
    fi
    echo "no processing"
  } | tee -a "${__DIRS:-}"/zzz_post-scripts.sh.log 2>&1
]]>
        </source>
      </script>
    </post-scripts>
    <!-- *** init-scripts ************************************************** -->
    <init-scripts t="list">
      <script t="map">
        <debug t="boolean">false</debug>
        <filename>zzz_init-scripts.sh</filename>
        <location/>
        <source><![CDATA[
  set -eu
  echo "zzz_init-scripts.sh"
  {
    if [ -n "${debug:-}" ] || [ -n "${debugout:-}" ]; then
      set -x
      export -p
    fi
    eject --verbose --cdrom || true
    zypper --quiet --non-interactive refresh || true
    zypper --quiet --non-interactive update  || true
  } | tee -a /var/admin/autoinst/zzz_init-scripts.sh.log 2>&1
]]>
        </source>
      </script>
    </init-scripts>
    <!-- *** scripts end *************************************************** -->
