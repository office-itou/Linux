gpgcheck=0

[rockylinux-chroot-Extras]
name=RockyLinux $releasever - $basearch - Extras
mirrorlist=https://mirrors.rockylinux.org/mirrorlist?arch=$basearch&repo=extras-$releasever$rltype
enabled=1
gpgcheck=0

