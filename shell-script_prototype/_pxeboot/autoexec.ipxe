prompt --key e --timeout ${optn-timeout} Press 'e' to open edit menu ... && set openmenu 1 ||
set hostname sv-ubuntu.workgroup
set srvraddr http://192.168.1.12
set selinuxs 
set otheropt fsck.mode=skip raid=noautodetect noeject
form                                    Configure Boot Options
item hostname                           Hostname
item srvraddr                           Server ip address
item selinuxs                           SELinux
item otheropt                           Other options
isset ${openmenu} && present ||
set ethrname ens160
set ipv4addr 192.168.1.1
set ipv4mask 255.255.255.0
set ipv4gway 192.168.1.254
set ipv4nsvr 192.168.1.254
form                                    Configure Network Options
item ethrname                           Interface
item ipv4addr                           IPv4 address
item ipv4mask                           IPv4 netmask
item ipv4gway                           IPv4 gateway
item ipv4nsvr                           IPv4 nameservers
isset ${openmenu} && present ||
set autoinst boot=live
set networks 
set language debian-installer/locale=ja_JP.UTF-8 keyboard-configuration/layoutcode=jp keyboard-configuration/modelcode=jp106
set ramsdisk root=/dev/ram0 load_ramdisk=1 ramdisk_size=1024000 overlay-size=80%
set isosfile boot=casper iso-url=${srvraddr}/isos/ubuntu-25.04-desktop-amd64.iso
form                                    Configure Autoinstall Options
item autoinst                           Auto install
item networks                           Network
item language                           Language
item ramsdisk                           RAM disk
item isosfile                           ISO file
isset ${openmenu} && present ||
set knladdr ${srvraddr}/imgs/ubuntu-desktop-25.04
set options ${autoinst} ${networks} ${language} ${ramsdisk} ${isosfile} ${selinuxs} ${otheropt}
echo Loading kernel and initrd ...
kernel ${knladdr}/casper/vmlinuz ${options} --- quiet || goto error
initrd ${knladdr}/casper/initrd || goto error
boot || goto error
exit

